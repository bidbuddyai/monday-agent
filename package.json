{
  "name": "monday-ai-assistant",
  "version": "1.1.0",
  "description": "AI-powered assistant for Monday.com with intelligent document parsing via Monday Code hosting",
  "keywords": [
    "monday",
    "ai",
    "assistant",
    "monday-code",
    "automation"
  ],
  "author": "Your Name",
  "license": "MIT",
  "main": "src/server/index.js",
  "scripts": {
    "start": "node src/server/prodServer.js",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "vite --config vite.config.js",
    "dev:server": "cross-env NODE_ENV=development nodemon --config nodemon.json",
    "build": "vite build --config vite.config.js",
    "test": "jest",
    "lint": "eslint \"src/**/*.{js,jsx}\"",
    "deploy": "npm run build && mapps code:push",
    "deploy:cdn": "mapps code:push --client-side",
    "manifest:import": "mapps manifest:import --manifestPath ./app-manifest.yml",
    "manifest:export": "mapps manifest:export --manifestPath ./manifest-export",
    "tunnel": "mapps tunnel:create -p 3000"
  },
  "dependencies": {
    "@mondaycom/apps-sdk": "^3.2.1",
    "axios": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "monday-sdk-js": "^0.5.6",
    "multer": "^2.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@mondaycom/apps-cli": "^4.8.0",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.3.1",
    "concurrently": "^9.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.35.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "vite": "^5.4.3"
  }
}
